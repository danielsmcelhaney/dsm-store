@model IEnumerable<StoreClient.Models.OrderModel>

@{
    ViewBag.Title = "Index";
}

<h2>Order Home</h2>

<table class="table">
    
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.totalPrice)
        </td>
        
    </tr>
}

</table>
